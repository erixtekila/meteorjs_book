{
    "title" : "Meteor in heaven",
    "author" : "Eric Priou aka erixtekila",
    "version": ">= 3.x.x",
    "links": {
        "sidebar":
        {
            "Centre de formation Mediabox": "http://www.mediabox.fr"
        }
        ,"sharing": {
            "weibo": null
        }
    },
    "plugins": [
        "-mathjax"
        ,"-infinitescroll"
        ,"-anchors"
        ,"jsbin"
        ,"sharing"
        ,"richquotes"
        ,"page-treeview"
        ,"-offline"
        ,"edit-link"
    ],
    "pluginsConfig":
    {
        "fontSettings":
        {
            "theme": "white",
            "family": "sans"
        }
        ,"sharing": {
            "facebook": false,
            "twitter": true,
            "google": false,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook", "google", "twitter",
                "weibo", "instapaper"
            ]
        }
        ,"page-treeview":
        {
            "style": "markdown",
            "copyright": "",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        }
        ,"edit-link":
        {
            "base": "https://github.com/erixtekila/meteorjs_book/edit/master",
            "label":
            {
                "en": "Edit This Page",
                "fr": "Editez depuis github"
            }
        }
    }
}
